    "homescreen.addInstructions" : {
      "comment" : "Separate instructions with a single new line",
      "extractionState" : "manual",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Drücke lange auf einen freien Bereich auf deinem Homebildschirm.\nTippe auf den Plus-Button oben links.\nSuche \"Peak\".\nWähle ein Widget aus und klicke auf \"Hinzufügen\".\nTippe auf das Widget, um es zu bearbeiten."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "On your Home Screen, long press in an empty space\nTap the plus button in the top left\nFind “Peak”\nPick a widget, and tap “Add Widget”\nTap the widget to edit it"
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "En su Pantalla de Inicio, pulsación larga en un espacio vacío\nToca el botón más en la parte superior izquierda\nEncuentra \"Peak\"\nElige un widget, y pulsa \"Agregar Widget\"\nToca el widget para editarlo"
          }
        }
      }
    },
