    "standBy.addInstructions" : {
      "comment" : "Separate instructions with a single new line",
      "extractionState" : "manual",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Aktiviere StandBy und wechsle zu einer Ansicht mit zwei Widgets nebeneinander.\nHalte ein Widget lange gedrückt.\nTippe auf den Plus-Button oben links.\nSuche \"Peak\".\nWähle ein Widget aus, und klicke auf \"Widget hinzufügen\".\nTippe auf das Widget, um es zu bearbeiten."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "Enter StandBy mode, and scroll to a view with two widgets side-by-side\nTap and hold on a widget\nTap the plus button in the top left\nFind “Peak”\nPick a widget, and tap “Add Widget”\nTap the widget to edit it"
          }
        },
        "es" : {
