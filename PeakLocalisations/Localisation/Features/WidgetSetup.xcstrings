    "standBy.editInstructions" : {
      "comment" : "Separate instructions with a single new line",
      "extractionState" : "manual",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Aktiviere den StandBy Modus und wechsle zu einer Ansicht mit einem Peak Widget.\nHalte das Widget lange gedrückt, bis eine vertikales Karussell erscheint.\nTippe auf das Widget im Karussell, um es zu bearbeiten."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Enter StandBy mode, and scroll to a view with a Peak widget\nTap and hold on the widget until a vertical carousel appears\nTap the widget in the carousel to edit it"
          }
        }
      }
    },
