    "lockscreen.addInstructions" : {
      "comment" : "Separate instructions with a single new line",
      "extractionState" : "manual",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Drücke lange auf einen freien Bereich auf deinem Sperrbildschirm.\nTippe auf \"Anpassen\".\nWähle den Sperrbildschirm aus.\nWähle ein Rechteck über oder unter der Zeit aus.\nSuche \"Peak\".\nWähle irgendein Widget aus um es hinzuzufügen (Hinweis: Du musst eventuell existierende Widgets entfernen, wenn nicht genug Platz vorhanden ist)\nTippe auf das Widget, um es zu bearbeiten."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "On your Lock Screen, long press in an empty space\nTap the “Customize” button\nSelect the Lock Screen\nSelect the rectangle above or below the time\nFind “Peak”\nTap any widget to add it (Note: You may need to remove existing widgets if there isn’t enough space)\nTap your newly added widget to edit it"
          }
        }
      }
    },
    "lockscreen.addInstructionsPad" : {
